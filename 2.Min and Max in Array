class Solution {
  public:
    pair<long long, long long> getMinMax(vector<long long int> arr) {
        long long min=LLONG_MAX;
        long long max=LLONG_MIN;
        for(int i=0;i<arr.size();i++){
            if(arr[i]<min) {min=arr[i];}
            if(arr[i]>max) {max=arr[i];}
        }
        return {min,max};
    }
};
